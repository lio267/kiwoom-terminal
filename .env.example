# Kiwoom REST API base URL (모의투자 기본값)
KIWOOM_BASE_URL=https://openapi-sandbox.kiwoom.com:9443

# Kiwoom OpenAPI+ REST credentials
# (client id/secret와 app key/secret이 동일하다면 같은 값을 입력하세요)
KIWOOM_APP_KEY=
KIWOOM_APP_SECRET=

# Optional: override TR IDs for 특정 API
# KIWOOM_TR_ID_HISTORICAL=FHKST03010100
# KIWOOM_TR_ID_QUOTE=FHKST01010100
# KIWOOM_CUSTOMER_TYPE=P

# Enable mock mode to 개발환경에서 목데이터 사용
MOCK_MODE=false
